{
    "builds": [
        {
            "src":"app.py",
            "use":"@vercel/python",
            "config": {
                "runtime": "python3.9",
                "requirementsPath": "requirements-vercel.txt",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src":"/(.*)", 
            "dest":"app.py"
        }
    ]
}